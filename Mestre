#include <stdio.h>

int main() {
    // Carta 1 - Cariacica
    char estado1 = 'E';
    char codigo1[4] = "E03";
    char cidade1[50] = "Cariacica";
    unsigned long int populacao1 = 348738;
    float area1 = 279.88;
    float pib1 = 12.5;
    int pontos1 = 6;
    float densidade1 = (float) populacao1 / area1;
    float pibPerCapita1 = pib1 / populacao1;
    float superPoder1 = populacao1 + area1 + pib1 + pontos1 + pibPerCapita1 + (1.0 / densidade1);

    // Carta 2 - Linhares
    char estado2 = 'E';
    char codigo2[4] = "E04";
    char cidade2[50] = "Linhares";
    unsigned long int populacao2 = 179755;
    float area2 = 3496.50;
    float pib2 = 8.7;
    int pontos2 = 5;
    float densidade2 = (float) populacao2 / area2;
    float pibPerCapita2 = pib2 / populacao2;
    float superPoder2 = populacao2 + area2 + pib2 + pontos2 + pibPerCapita2 + (1.0 / densidade2);

    // Saída de comparação
    printf("=== Comparacao entre as cartas ===\n");
    printf("Populacao: Carta %d venceu\n", (populacao1 > populacao2) ? 1 : 2);
    printf("Area: Carta %d venceu\n", (area1 > area2) ? 1 : 2);
    printf("PIB: Carta %d venceu\n", (pib1 > pib2) ? 1 : 2);
    printf("Numero de Pontos Turisticos: Carta %d venceu\n", (pontos1 > pontos2) ? 1 : 2);
    printf("Densidade Populacional: Carta %d venceu\n", (densidade1 < densidade2) ? 1 : 2);
    printf("PIB per Capita: Carta %d venceu\n", (pibPerCapita1 > pibPerCapita2) ? 1 : 2);
    printf("Super Poder: Carta %d venceu\n", (superPoder1 > superPoder2) ? 1 : 2);

    return 0;
}
